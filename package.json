{
  "name": "@slonoed/jsref",
  "description": "JavaScript refactoring language server",
  "version": "0.6.1",
  "main": "lib/index.js",
  "license": "WTFPL",
  "repository": "https://github.com/slonoed/jsref",
  "bin": {
    "jsref": "./lib/bin/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "flow-watch": "fswatch -o ./src/ | xargs -n1 -I{} sh -c 'clear; printf \"\\033[3J\" && flow'",
    "lint": "npx eslint src",
    "test": "npx flow && npm run lint && npx jest src",
    "test-watch": "npx jest --watch src",
    "build": "npx babel -d lib src",
    "build-watch": "npx babel --watch -d lib src"
  },
  "dependencies": {
    "babel-generator": "^6.26.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-template": "^6.26.0",
    "babel-types": "^6.26.0",
    "babylon": "^6.18.0",
    "babylon-walk": "^1.0.2",
    "log4js": "^2.6.0",
    "prettier": "^1.12.1",
    "vscode-jsonrpc": "^3.6.2",
    "vscode-languageserver": "^4.2.1",
    "vscode-languageserver-types": "^3.8.2",
    "vscode-uri": "^1.0.5",
    "yargs": "^11.0.0"
  },
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-rulesdir": "^0.1.0",
    "flow-bin": "^0.72.0",
    "jest": "^22.4.3",
    "sinon": "^5.0.7"
  }
}
